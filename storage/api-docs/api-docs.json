                "summary": "Find the anagrams of the given word.",
                "operationId": "f07f1cb5acd8476771e8e68b4981cf3d",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "word": {
                                        "type": "string"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "word": "kabe"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "examples": {
                                    "result": {
                                        "summary": "",
                                        "value": {
                                            "success": "true",
                                            "data": [
                                                {
                                                    "id": 11355,
                                                    "word": "ebak",
                                                    "hash": "abek"
                                                },
                                                {
                                                    "id": 33269,
                                                    "word": "kabe",
                                                    "hash": "abek"
                                                }
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
